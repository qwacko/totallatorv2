{
  "name": "pocketbase-sveltekit-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "dev:backend": "cd ../pb && modd",
    "backend": "cd ../pb && ./pocketbase serve --publicDir=../sk/build",
    "build": "vite build",
    "build:backend": "cd ../pb && go build",
    "typegen": "pocketbase-typegen --db ../pb/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@iconify/svelte": "^3.1.4",
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.22.3",
    "@types/node": "^20.4.5",
    "autoprefixer": "^10.4.14",
    "playwright": "^1.36.2",
    "pocketbase-typegen": "^1.1.11",
    "postcss": "^8.4.27",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.64.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "type": "module",
  "dependencies": {
    "@melt-ui/svelte": "^0.27.1",
    "pocketbase": "^0.15.3",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "typed-pocketbase": "0.0.6"
  }
}
